<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewData="clr-namespace:Mmu.Was.WpfUI.Areas.Word.ViewData">

    <DataTemplate x:Key="ResultCheckPassed" DataType="{x:Type viewData:RuleCheckResultViewData}">
        <Image Width="15" Height="15" Source="{Binding RulePassedImageSource, Mode=OneWay}" />
    </DataTemplate>

    <DataTemplate x:Key="ResultCheckReport" DataType="{x:Type viewData:RuleCheckResultViewData}">
        <TextBox TextWrapping="Wrap" Text="{Binding ResultOverview, Mode=OneWay}" />
    </DataTemplate>

</ResourceDictionary>